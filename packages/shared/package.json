{
  "name": "@bookwise/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for the Bookwise project",
  "type": "module",
  "exports": {
    "./enums": {
      "default": "./dist/enums.js",
      "types": "./dist/enums.d.ts"
    },
    "./schemas/*": {
      "default": "./dist/schemas/*.js",
      "types": "./dist/schemas/*.d.ts"
    },
    "./dtos/*": {
      "default": "./dist/dtos/*.js",
      "types": "./dist/dtos/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint"
  },
  "dependencies": {
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@bookwise/eslint-config": "workspace:*",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
